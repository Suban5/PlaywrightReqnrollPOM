{
  "dotnet.defaultSolution": "PlaywrightReqnrollFramework.sln",
  "dotnet-test-explorer.testArguments": "--settings PlaywrightReqnrollFramework.runsettings",
  "dotnet.unitTests.runSettingsPath": "${workspaceFolder}/PlaywrightReqnrollFramework.runsettings",
  "cucumberautocomplete.steps": [
    "PlaywrightReqnrollFramework/**/*.cs"
  ],
  "cucumberautocomplete.syncfeatures": "PlaywrightReqnrollFramework/**/*.feature",
  "cucumberautocomplete.strictGherkinCompletion": true,
  "cucumberautocomplete.smartSnippets": true,
  "cucumberautocomplete.stepsInvariants": true,
  "files.exclude": {
    "**/*.feature.cs": true
  }
}